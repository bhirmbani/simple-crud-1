<% include ./partials/header %>

<h1><%= title %></h1>

<table class="ui striped table">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% moviers.forEach(movier => { %>
        <tr>
          <td><%= movier.first_name %> </td>
          <td><%= movier.last_name %></td>
          <td><%= movier.email %></td>
        </tr>
        
        <% }) %>
      </tbody>
    </table>


<% include ./partials/footer %>